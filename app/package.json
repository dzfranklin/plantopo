    "@tailwindcss/typography": "^0.5.13",
    "@turf/along": "^7.0.0",
    "@turf/bbox": "^7.0.0",
    "@turf/distance": "^7.0.0",
    "@turf/helpers": "^7.0.0",
    "@turf/length": "^7.0.0",
    "@turf/simplify": "^7.0.0",
    "@workos-inc/authkit-nextjs": "^0.6.1",
    "framer-motion": "^11.2.10",
