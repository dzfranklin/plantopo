{
  "name": "plantopo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https -p 443",
    "build": "npm run build:dependency-report && next build",
    "start": "npm run build && next start",
    "lint": "prettier . --check && next lint",
    "build:dependency-report": "npx license-checker --json | jq 'to_entries | sort_by(.key) | map({name: .key, publisher: .value.publisher, licenses: .value.licenses, repository: .value.repository})' > dependencyReport.json"
  },
